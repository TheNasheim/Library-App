<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ProgramController">
    <children>
        <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>


            <TableView fx:id="tvListofObjects" layoutX="14.0" layoutY="85.0" onMouseClicked="#tvListofObjects_Click" prefHeight="390.0" prefWidth="580.0" />
            <Button fx:id="btnRead" layoutX="603.0" layoutY="110.0" mnemonicParsing="false" onMouseClicked="#onReadMore_Click" prefHeight="40.0" prefWidth="90.0" text="  Read more &#10;   Info" textAlignment="CENTER" />
            <Button fx:id="btnBorrow" layoutX="603.0" layoutY="340.0" mnemonicParsing="false" onMouseClicked="#onBorrow_Click" prefHeight="25.0" prefWidth="90.0" text="Borrow Book" />
            <Button fx:id="btnReturn" layoutX="603.0" layoutY="370.0" mnemonicParsing="false" onMouseClicked="#onReturn_Click" prefHeight="25.0" prefWidth="90.0" text="Return Book" />
            <TextField fx:id="txtSearch" layoutX="304.0" layoutY="55.0" prefHeight="25.0" prefWidth="232.0" />
            <Button layoutX="542.0" layoutY="55.0" mnemonicParsing="false" onMouseClicked="#onSearch_Click" text="Search" />
            <Label fx:id="lblStatus" layoutX="14.0" layoutY="478.0" text="Status:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
</GridPane>
